{
  "name": "@toruslabs/torus-embed",
  "version": "1.21.0",
  "description": "Embed script for Torus",
  "main": "dist/torus.cjs.js",
  "module": "dist/torus.esm.js",
  "unpkg": "dist/torus.umd.min.js",
  "jsdelivr": "dist/torus.umd.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torusresearch/torus-embed.git"
  },
  "scripts": {
    "start": "rimraf dist/ && torus-scripts start --name=torus",
    "dev": "rimraf dist/ && cross-env NODE_ENV=development torus-scripts build --name=torus",
    "build": "rimraf dist/ && cross-env NODE_ENV=production torus-scripts build --name=torus",
    "prepack": "npm run build",
    "lint:ts": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "dependencies": {
    "@metamask/obs-store": "^7.0.0",
    "@toruslabs/fetch-node-details": "^5.0.1",
    "@toruslabs/http-helpers": "^2.2.0",
    "@toruslabs/openlogin-jrpc": "^1.5.0",
    "@toruslabs/torus.js": "^5.0.1",
    "create-hash": "^1.2.0",
    "end-of-stream": "^1.4.4",
    "eth-rpc-errors": "^4.0.3",
    "events": "^3.3.0",
    "fast-deep-equal": "^3.1.3",
    "is-stream": "^3.0.0",
    "lodash.merge": "^4.6.2",
    "loglevel": "^1.8.0",
    "once": "^1.4.0",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@toruslabs/eslint-config-typescript": "^0.2.0",
    "@toruslabs/torus-scripts": "^0.4.3",
    "@types/create-hash": "^1.2.2",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^14.17.0",
    "@types/once": "^1.4.0",
    "@types/readable-stream": "^2.3.13",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "cross-env": "^7.0.3",
    "empty-module": "0.0.2",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "mocha": {
    "timeout": 0
  },
  "keywords": [
    "torus",
    "wallet",
    "ERC20",
    "Ethereum",
    "embed",
    "login",
    "OAuth",
    "crypto"
  ],
  "author": "Torus Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/torusresearch/torus-embed/issues"
  },
  "homepage": "https://github.com/torusresearch/torus-embed#readme",
  "lint-staged": {
    "!(*d).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json, md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.x"
  }
}
